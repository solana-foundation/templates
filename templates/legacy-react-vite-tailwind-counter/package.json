{
  "name": "legacy-react-vite-tailwindtailwind-counter",
  "description": "React + Vite + Tailwind + Anchor Counter program",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "anchor": "cd anchor && anchor",
    "anchor-build": "cd anchor && anchor build",
    "anchor-localnet": "cd anchor && anchor localnet",
    "anchor-test": "cd anchor && anchor test",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "ci": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.30.1",
    "@solana/spl-token": "0.4.12",
    "@solana/wallet-adapter-base": "0.9.23",
    "@solana/wallet-adapter-react": "0.15.35",
    "@solana/wallet-adapter-react-ui": "0.9.35",
    "@solana/web3.js": "1.98.0",
    "@tabler/icons-react": "^3.30.0",
    "@tanstack/react-query": "^5.66.9",
    "daisyui": "^4.12.24",
    "jotai": "^2.12.1",
    "node-stdlib-browser": "^1.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild": "0.25.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "create-solana-dapp": {
    "rename": {
      "counter": {
        "to": "{{name}}",
        "paths": [
          "anchor",
          "src"
        ]
      },
      "scaffold": {
        "to": "{{name}}",
        "paths": [
          "src"
        ]
      }
    },
    "instructions": [
      "Run Anchor commands:",
      "+{pm} run anchor build | test | localnet | deploy"
    ],
    "versions": {
      "anchor": "0.30.1",
      "solana": "1.18.0"
    }
  }
}
